<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
<system
    xmlns="urn:example:system"
    xmlns:arch="urn:ietf:params:xml:ns:yang:ietf-yang-architecture">

  <hostname arch:origin="arch:dynamic">bar</hostname>

  <interface arch:origin="arch:static">
    <name>eth0</name>
    <mtu arch:origin="arch:default">1500</mtu>
    <auto-negotiate-speed/>
    <speed arch:origin="arch:system">100</speed>
    <in-pkts>42</in-pkts>
    <address>
      <ip>2001:db8::10</ip>
      <prefix-length>32</prefix-length>
    </address>
    <address arch:origin="arch:dynamic">
      <ip>2001:db8::1:100</ip>
      <prefix-length>32</prefix-length>
    </address>
  </interface>

  <interface arch:origin="arch:system">
    <name>lo0</name>
    <mtu>65536</mtu>
    <in-pkts>4711</in-pkts>
    <address>
      <ip>::1</ip>
      <prefix-length>128</prefix-length>
    </address>
  </interface>

</system>
</data>
