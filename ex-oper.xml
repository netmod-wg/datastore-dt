<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
<system
    xmlns="urn:example:system"
    xmlns:ds="urn:ietf:params:xml:ns:yang:ietf-datastores">

  <hostname ds:origin="ds:dynamic">bar</hostname>

  <interface ds:origin="ds:static">
    <name>eth0</name>
    <auto-negotiation>
      <enabled ds:origin="ds:default">true</enabled>
      <speed>1000</speed>
    </auto-negotiation>
    <speed>100</speed>
    <address>
      <ip>2001:db8::10</ip>
      <prefix-length>32</prefix-length>
    </address>
    <address ds:origin="ds:dynamic">
      <ip>2001:db8::1:100</ip>
      <prefix-length>32</prefix-length>
    </address>
  </interface>

  <interface ds:origin="ds:system">
    <name>lo0</name>
    <address>
      <ip>::1</ip>
      <prefix-length>128</prefix-length>
    </address>
  </interface>

</system>
</data>
